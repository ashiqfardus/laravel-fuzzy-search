{
    "name": "ashiqfardus/laravel-fuzzy-search",
    "description": "A powerful, zero-config fuzzy search package for Laravel with fluent API",
    "keywords": ["laravel", "fuzzy", "search", "eloquent", "query-builder", "levenshtein", "soundex", "trigram", "typo-tolerance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Md Asikul Islam",
            "email": "ashiqfardus@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ashiqfardus\\LaravelFuzzySearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashiqfardus\\LaravelFuzzySearch\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "benchmark": "php artisan fuzzy-search:benchmark"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashiqfardus\\LaravelFuzzySearch\\FuzzySearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

